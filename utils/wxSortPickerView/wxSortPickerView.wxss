.wxSortPickerViewTemTags{
  position: fixed;
  top: 20%;
  right: 0;
  color: gray;
}
.wxSortPickerViewTemTag{
  text-align: center;
  padding: 3px;
  font-size: 12px;
}
.wxSortPickerViewFixedTag{
  position: fixed;
  width: 100%;
}

.wxSortPickerViewTag{
  background-color: #efeff4;
  font-size: 12px;
  font-weight: bold;
  color: #000;
  height: 24px;
  line-height: 24px;
  padding: 0 5px;
}

.wxSortPickerViewItem{
  height: 55px;
  line-height: 55px;
  border-bottom: 1px solid #eee;
  margin-left: 15px;
  background-color: #fff;
}

.wxSortPickerViewItem image{
  width: 40px;
  height: 40px;
  vertical-align: middle;
}
.wxSortPickerViewItem text{
  font-size: 14px;
  margin-left: 10px;
}